@model IEnumerable<QLBANSACH.Models.CHUDE>
<a class="opened" href="#">Chủ đề sách</a>
<div>
    @foreach (var item in Model)
    {
        <div class="list-chude">
            <a href="@Url.Action("SPTheochude", "BookStore", new {id= item.MaCD})" class="list-group-item">
                <p style="float:left;width:190px">@item.TenChuDe</p><i class="fa-solid fa-chevron-right" ></i>
            </a>
        </div>
    }

</div>
